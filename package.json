  "scripts": {
    "prepare": "husky install",
    "dev": "vite --port=3000",
    "build": "vite build",
    "preview": "vite preview --host --port 3000",
    "test": "vitest",
    "cy:open": "cypress open",
    "cy:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cy:open",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cy:headless",
    "lint": "eslint --ext .js,.ts,.tsx --config .eslintrc.js --ignore-path .eslintignore --fix --cache",
    "stylelint": "stylelint \"**/*.{css,scss,sass,less}\" \"**/min.css/**\" --config ./stylelint.config.js --allow-empty-input -i ./.stylelintignore --fix -f verbose",
    "prettier": "prettier \"**/*.+(js|json|ts|tsx)\" --ignore-unknown --ignore-path ./.prettierignore --list-different --cache",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check --list-different",
    "types:check": "tsc --project tsconfig.json --pretty --noEmit",
    "generate:package": "plop package",
    "generate:component": "plop component",
    "storybook:clean": "rm -rf node_modules/.cache/storybook",
    "storybook:open": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "pnpx serve storybook-static"
  },
